  DEFINES   += -DNDEBUG
  INCLUDES  += 
  CPPFLAGS  += -MMD -MP $(DEFINES) $(INCLUDES)
  CFLAGS    += $(CPPFLAGS) $(ARCH) -O3 -ffast-math -Wall -Wno-sign-compare
  CXXFLAGS  += $(CFLAGS) 
  LDFLAGS   += -s -static -static-libstdc++
  LIBS      += -lm -lrt
  RESFLAGS  += $(DEFINES) $(INCLUDES) 
  LDDEPS    += 
  LINKCMD    = $(AR) -rcs $(TARGET) $(OBJECTS)
